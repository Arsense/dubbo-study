<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:AresServer="http://www.ares-remoting.com/schema/ares-service"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
 http://www.ares-remoting.com/schema/ares-service http://www.ares-remoting.com/schema/ares-service.xsd">



    <bean id="testService" class="com.remote.test.TestServiceImpl"/>

    <AresServer:service id="testServiceRegister"
                        interface="com.remote.test.TestService"
                        ref = "testService"
                        port="8081"/>
</beans>